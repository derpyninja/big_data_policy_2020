  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# all rows where the value of a is smaller than 10:\n",
    "print(df[df[\"year\"] < 1945])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
